{
  "name": "sport_bookmakers_scraper",
  "version": "0.1.0",
  "description": "Sport bookmakers scraper",
  "main": "dist/bookmakers/scrapLuckia.js",
  "type": "module",
  "author": "Juan David Tabares Arce",
  "license": "MIT",
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juandata/sport_bookmakers_scraper.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build-ts": "npm run clean && tsc",
    "serve-replit": "node dist/bookmakers/luckia/scrapLuckia.js",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "node --inspect-brk node_modules/.bin/tsx src/bookmakers/luckia/scrapLuckia.ts",
    "serve": "npx tsx src/bookmakers/luckia/scrapLuckia.ts",
    "inspect": "node --inspect-brk src/bookmakers/luckia/scrapLuckia.ts",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose --passWithNoTests",
    "watch-node": "nodemon src/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "@babel/parser": "^7.26.10",
    "@babel/types": "^7.26.10",
    "@types/jest": "^29.5.14",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "copyfiles": "^2.1.0",
    "expect": "^29.7.0",
    "it-each": "^0.4.0",
    "jest": "^29.7.0",
    "mocha": "^6.1.4",
    "mochawesome": "^4.0.0",
    "pre-commit": "latest",
    "pretty-format": "^29.7.0",
    "puppeteer": "^24.1.0",
    "puppeteer-debug": "^1.0.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "rimraf": "^2.6.3",
    "selenium-webdriver": "^4.28.1",
    "ts-node": "^10.9.2",
    "typescript": ">=4.3 <6"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/chai": "^4.1.7",
    "@types/node": "^16.11.20",
    "@types/puppeteer": "^1.12.4",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "ts-jest": "^29.2.5",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "tsx": "^4.19.2",
    "typescript-eslint": "^8.26.1"
  }
}
